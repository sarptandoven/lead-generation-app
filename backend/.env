# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/leadgen

# LinkedIn Credentials
LINKEDIN_USERNAME=your_linkedin_username
LINKEDIN_PASSWORD=your_linkedin_password

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# Application Settings
APP_SECRET_KEY=your_secret_key_here
ENVIRONMENT=development
DEBUG=True

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=30
MAX_REQUESTS_PER_HOUR=1000

# Proxy Settings (Optional)
PROXY_LIST=proxy1.example.com:8080,proxy2.example.com:8080

# Email Verification API (Optional)
EMAIL_VERIFICATION_API_KEY=your_email_verification_api_key

# Logging
LOG_LEVEL=INFO
LOG_FILE=app.log
